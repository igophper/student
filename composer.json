{
  "name": "igophper/student",
  "description": "A student package",
  "keywords": [
    "student"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "igophper",
      "email": "admin@jialigu.cn",
      "homepage": "https://jialigu.cn",
      "role": "Developer"
    }
  ],
  "type": "library",
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Igophper\\Student\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Igophper\\Student\\ServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests"
  }
}